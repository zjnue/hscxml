<?xml version="1.0"?>
 <!-- in the ECMA data model, test that processor creates an ECMAScript DOM object
 _event.data when receiving XML in an event -->
 
 <scxml xmlns="http://www.w3.org/2005/07/scxml" 
   xmlns:conf="http://www.w3.org/2005/scxml-conformance" initial="s0" version="1.0" datamodel="ecmascript">

  
<state id="s0">
   <onentry>
    <send event="foo">
    <content>
<books xmlns="">
<book title="title1"/>
<book title="title2"/>
</books>
</content>
    </send>
  </onentry>
  <transition event="foo" cond="_event.data.getElementsByTagName('book')[1].getAttribute('title') == 'title2'"
                     conf:targetpass=""/>
  <transition event="*" conf:targetfail=""/>
  </state>
  
    
<conf:pass/>
<conf:fail/>  

</scxml>